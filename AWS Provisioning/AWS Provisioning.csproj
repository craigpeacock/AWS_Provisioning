<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>AWS_Provisioning</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.IoT" Version="3.7.307.17" />
    <PackageReference Include="AWSSDK.SecurityToken" Version="3.7.300.71" />
    <PackageReference Include="AWSSDK.SSO" Version="3.7.300.70" />
  </ItemGroup>

</Project>